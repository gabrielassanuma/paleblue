<%- if controller_name != 'sessions' %>
  <%= link_to "Connect Wallet", new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "New Wallet", new_registration_path(resource_name), class: 'btn btn-ghost'%><br />
<% end %>
